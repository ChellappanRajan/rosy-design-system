{
  "name": "@rosy-design-system/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng-dev": "npx nx run rosy-design-system:serve",
    "ng-test": "npx nx run rosy-design-system-e2e:e2e",
    "target-check": "npx nx show project rosy-design-system-e2e --web",
    "vue-dev": "npx nx run vue-playground:serve",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "lint:eslint": "eslint --ext .ts,.html . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts,.html . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "storybook": "nx run rosy-design-system-app:storybook",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~18.2.0",
    "@angular/common": "~18.2.0",
    "@angular/compiler": "~18.2.0",
